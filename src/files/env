# .env file

# Marketo Credentials
MARKETO_BASE_URL=https://XXX-XXX-XXX.mktorest.com
MARKETO_CLIENT_ID=your_marketo_client_id
MARKETO_CLIENT_SECRET=your_marketo_client_secret
MARKETO_ROOT_FOLDER_ID=34 # The Images & Files ID of Marketo

# HubSpot Credentials
HUBSPOT_ACCESS_TOKEN=your_hubspot_private_app_token
# Optional: Specify a root folder in HubSpot to migrate into
HUBSPOT_TARGET_ROOT_FOLDER_ID=null # Use null to migrate to the top level, or provide an ID